{
    "servers": {
        "d1-agent": {
            "type": "sse",
            "command": "npx",
            "args": [
                "mcp-remote",
                "http://localhost:8787/mcp"// or remote-mcp-server.your-account.workers.dev/mcp
            ]
        },
        "cf-docs": {
            "type": "sse",
            "command": "npx",
            "args": [
                "mcp-remote",
                "https://docs.mcp.cloudflare.com/sse"
            ]
        },
        "cf-graphql": {
            "type": "sse",
            "command": "npx",
            "args": [
                "mcp-remote",
                "https://graphql.mcp.cloudflare.com/sse"
            ]
        },
        "subgraph": {
            "command": "npx",
            "args": ["mcp-remote", "--header", "Authorization:${AUTH_HEADER}", "https://subgraphs.mcp.thegraph.com/sse"],
            "env": {
                "AUTH_HEADER": "Bearer e74e6acaa5e94a4917afc26650ff00bb"
            }
        }
    }
}
