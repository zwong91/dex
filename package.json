{
  "name": "entysquare",
  "private": true,
  "version": "1.0.0",
  "description": "EntySquare - Modern Multi-chain Decentralized Exchange Platform with Concentrated Liquidity",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "version:prerelease": "npm version prerelease",
    "release:prepare": "npm run lint && npm run build",
    "release:check": "npm audit --audit-level=moderate",
    "deploy:cloudflare": "npm run build && npx wrangler pages deploy dist --project-name=entysquare-dex",
    "deploy:preview": "npm run build && npx wrangler pages deploy dist --project-name=entysquare-dex --compatibility-date=2024-01-17"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@lb-xyz/sdk": "5.0.14",
    "@lb-xyz/sdk-core": "2.0.13",
    "@lb-xyz/sdk-v2": "3.0.28",
    "@mui/icons-material": "^7.1.2",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.1.2",
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@tailwindcss/postcss": "^4.1.11",
    "@tanstack/react-query": "^5.81.5",
    "axios": "^1.10.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "ethers": "^6.14.4",
    "lucide-react": "^0.525.0",
    "nanoid": "^5.1.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-multi-carousel": "^2.8.5",
    "react-router-dom": "^7.6.3",
    "sonner": "^2.0.5",
    "viem": "^2.31.4",
    "wagmi": "^2.12.29",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@types/nanoid": "^2.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.30.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^16.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^7.0.0"
  }
}
